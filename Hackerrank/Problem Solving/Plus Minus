#include <bits/stdc++.h>
using namespace std;

int main() {
	int n;
	cin>>n;
	int *arr=new int[n];
	float zero=0,positive=0,negative=0;
	for(int i=0;i<n;i++)
	{
		cin>>arr[i];
		if(arr[i]==0)
		{
			zero++;
		
		}
		else if (arr[i]>0)
		{
			positive++;
		}
		else
		{
			negative++;
		}
	}
	cout<<(fixed)<<setprecision(6)<<positive/float(n)<<endl<<(fixed)<<setprecision(6)<<negative/float(n)<<endl<<(fixed)<<setprecision(6)<<zero/float(n)<<endl;
	
	
	
	return 0;
}
