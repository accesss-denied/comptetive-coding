#include <iostream>
#include<bits/stdc++.h>
using namespace std;

void fucn(int *arr,int *arr2,int i)
{
//	cout<<sizeof(arr)/2<<endl;
//cout<<i<<endl;
	if(i==sizeof(arr)/2)
	{
		for(int j=0;j<sizeof(arr2)/2;j++)
		{
			if(arr2[j]!=-2)
			{
			cout<<arr2[j];
			}
			
		}
		//cout<<"Ashu"<<endl;
		cout<<endl;
		return;
	}
	else
	{
		arr2[i]=NULL;
		fucn (arr,arr2,i+1);
		
		arr2[i]=arr[i];
			fucn (arr,arr2,i+1);
			return;
	}
}

int main() {
int n;
cin>>n;
int *arr=new int[n];
int *arr2=new int[n];
for(int i=0;i<n;i++)
{
	cin>>arr[i];
	arr2[i]=-2;
	
}

fucn(arr,arr2,0);
	return 0;
}
